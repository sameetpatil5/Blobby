{
    "version": 2,
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "run.py"
        },
        {
            "src": "/(.*)",
            "headers": {"Access-Control-Allow-Origin": "*"},
            "continue": true
        }
    ]
}